{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "g++ build and debug main",
      "type": "cppdbg",
      "request": "launch",
      "program": "build/main.out",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "linux": {
        "environment": []
      },
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "g++ build main",
      "miDebuggerPath": "/usr/bin/gdb"
    }
  ]
}